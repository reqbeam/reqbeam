{
  "name": "API Test Collection",
  "description": "Example collection for testing API endpoints",
  "requests": [
    {
      "name": "Get Users",
      "method": "GET",
      "url": "https://jsonplaceholder.typicode.com/users",
      "expect": {
        "status": 200,
        "contains": "email",
        "responseTime": 5000
      }
    },
    {
      "name": "Get Single User",
      "method": "GET",
      "url": "https://jsonplaceholder.typicode.com/users/1",
      "expect": {
        "status": 200,
        "jsonPath": {
          "id": 1,
          "username": "Bret"
        }
      }
    },
    {
      "name": "Create Post",
      "method": "POST",
      "url": "https://jsonplaceholder.typicode.com/posts",
      "headers": {
        "Content-Type": "application/json"
      },
      "body": {
        "title": "Test Post",
        "body": "This is a test post",
        "userId": 1
      },
      "expect": {
        "statusRange": [200, 201],
        "contains": "Test Post"
      }
    },
    {
      "name": "Get Posts",
      "method": "GET",
      "url": "https://jsonplaceholder.typicode.com/posts?userId=1",
      "expect": {
        "status": 200,
        "contains": "userId"
      }
    },
    {
      "name": "Update Post",
      "method": "PUT",
      "url": "https://jsonplaceholder.typicode.com/posts/1",
      "headers": {
        "Content-Type": "application/json"
      },
      "body": {
        "id": 1,
        "title": "Updated Title",
        "body": "Updated body",
        "userId": 1
      },
      "expect": {
        "status": 200,
        "contains": "Updated Title"
      }
    },
    {
      "name": "Delete Post",
      "method": "DELETE",
      "url": "https://jsonplaceholder.typicode.com/posts/1",
      "expect": {
        "status": 200
      }
    }
  ]
}

